        "scripts:build": "set NODE_ENV=production&&node deploy.js",
        "scripts:watch": "webpack-dev-server --inline  --port 3040",
        "lint": "echo skip",
        "lint:fix": "eslint js/. --ext .js --fix",
        "start": "node src/server.js production"
    },
    "nyc": {
