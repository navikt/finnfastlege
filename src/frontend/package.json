{
    "name": "fastlegefront",
    "version": "0.0.1",
    "description": "Digitalisering av sykefraværsoppfølging",
    "main": "index.js",
    "test": "npm run test",
    "scripts": {
        "dev": "title fastlegefront && npm-run-all images static:build:local scripts:watch",
        "build": "npm-run-all images static:build scripts:build",
        "nais-build": "npm run build",
        "test:watch": "title fastlegefront && cross-env NODE_ENV=test nyc mocha --watch -G --require ignore-styles --compilers js:babel-core/register --require babel-polyfill ./test/setup.js --recursive test",
        "test": "cross-env NODE_ENV=test nyc mocha -G --require ignore-styles --compilers js:babel-core/register --require babel-polyfill ./test/setup.js --recursive test",
        "posttest": "nyc report --reporter=html --reporter=lcov",
        "static:build:local": "title fastlegefront && node build-html.js html/fastlegefront.mustache local",
        "static:build": "node build-html.js html/fastlegefront.mustache prod",
        "scripts:build": "set NODE_ENV=production&&node deploy.js",
        "scripts:watch": "title fastlegefront && webpack-dev-server --inline  --port 3040",
        "lint": "echo skippp",
        "lint:fix": "eslint js/. --ext .js --fix",
        "images": "cp -R img ../main/webapp"
    },
    "nyc": {
        "require": [
            "babel-register"
        ],
        "sourceMap": false,
        "instrument": false
    },
    "author": "Team DigiSyfo <digisyfo@nav.no>",
